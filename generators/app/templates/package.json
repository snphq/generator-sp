{
  "name": "projects",
  "private": "true",
  "scripts": {
    "tdd": "karma start",
    "test": "npm run lint && karma start --single-run",
    "lint": "npm run lintxo && npm run lintcoffee",
    "lintcoffee": "coffeelint -f .coffeelintrc app/scripts",
    "lintxo": "xo *.js && xo app/scripts/**/*.js webpack/**/*.js --esnext",
    "dev": "MODE=DEBUG node ./run-dev.js",
    "dist": "MODE=DIST node ./run-build.js",
    "prod": "MODE=PROD node ./run-build.js",
    "precommit": "npm run test"
  },
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "backbone": "1.1.2",
    "backbone-mixin": "lexich/backbone-mixin",
    "backbone.epoxy": "^1.3.4",
    "bootstrap": "^3.3.6",
    "cookies": "^0.5.1",
    "jquery": "^1.12.0",
    "lodash": "^3.10.1",
    "reset.css": "^2.0.2",
    "simple-blocks": "lexich/simple-blocks",
    "sp-utils-bootstrapmodal": "lexich/sp-utils-bootstrapmodal",
    "sp-utils-gaconstructor": "lexich/sp-utils-gaconstructor",
    "sp-utils-middleware": "lexich/sp-utils-middleware",
    "sp-utils-paginatecollection": "lexich/sp-utils-paginatecollection",
    "sp-utils-serverclient": "lexich/sp-utils-serverclient"
  },
  "devDependencies": {
    "babel-core": "^6.4.0",
    "babel-loader": "^6.2.1",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "chai": "^3.5.0",
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.14.2",
    "coffeelint-loader": "^0.1.1",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^2.15.0",
    "husky": "^0.10.2",
    "ibrik": "^2.0.0",
    "ibrik-instrumenter-loader": "^0.1.1",
    "imports-loader": "^0.6.5",
    "jade": "^1.11.0",
    "jade-loader": "^0.8.0",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chai-sinon": "^0.1.5",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-mocha": "^0.2.1",
    "karma-notify-reporter": "^0.1.1",
    "karma-nyan-reporter": "^0.2.3",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-phantomjs-shim": "^1.2.0",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "modernizr": "^3.3.1",
    "modernizr-webpack-plugin": "^1.0.5",
    "node-sass": "^3.4.2",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.3",
    "postcss-assets": "^4.0.1",
    "postcss-bem": "^0.4.1",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.0",
    "postcss-size": "^1.0.0",
    "postcss-svgo": "^2.1.2",
    "sass-loader": "^3.1.2",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "stylelint": "^4.3.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.11",
    "webpack-dev-server": "^1.14.1",
    "xo": "^0.13.0",
    "xo-loader": "^0.6.0"
  },
  "engines": {
    "node": ">=0.12.0"
  },
  "xo": {
    "envs": [
      "node",
      "mocha",
      "browser"
    ],
    "esnext": "true",
    "globals": [
      "$",
      "_",
      "expect",
      "Modernizr",
      "common",
      "BUILD_MODE"
    ],
    "space": "true",
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "camelcase": [
        1,
        {
          "properties": "always"
        }
      ]
    }
  }
}
